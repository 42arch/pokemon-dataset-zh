    "flavor_texts": [
        {
            "name": "第一世代",
            "versions": [
                {
                    "name": "红",
                    "group": "宝可梦 红／绿／蓝",
                    "text": "别名谜行草。据说到了晚上，会用２条根走上长达３００米的距离。"
                },
                {
                    "name": "绿",
                    "group": "宝可梦 红／绿／蓝",
                    "text": "别名谜行草。据说到了晚上，会用２条根走上长达３００米的距离。"
                },
                {
                    "name": "蓝",
                    "group": "宝可梦 红／绿／蓝",
                    "text": "白天它的脸保持在地下。晚上，它四处播种。"
                },
                {
                    "name": "皮卡丘",
                    "group": "宝可梦 皮卡丘",
                    "text": "要是以为它是普通的草而将它拔起，就会听到它的叫声。这让人感到莫名的恐怖。"
                }
            ]
        },
        {
            "name": "第二世代",
            "versions": [
                {
                    "name": "金",
                    "group": "宝可梦 金／银",
                    "text": "沐浴月光醒来后，会到处走动，白天则一动不动待在地里。"
                },
                {
                    "name": "银",
                    "group": "宝可梦 金／银",
                    "text": "沐浴月光就会开始活动。为了四处散播种子会在夜晚到处走动。"
                },
                {
                    "name": "水晶版",
                    "group": "宝可梦 水晶版",
                    "text": "白天会为躲避太阳而钻入凉爽的地下。会通过沐浴月光成长。"
                }
            ]
        },
        {
